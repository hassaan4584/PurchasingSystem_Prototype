<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <p class="bg-success">Purchase Requsation</p>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" role="form">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">PR id</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" value="1">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">catagory</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="Furniture">
                        </div>
                    </div>
                  
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Product 1</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="name: chairs  Quantity: 15">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Product 2</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="name: tables  Quantity: 15 ">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Product 3</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="name:  White Boards   Quantity: 15">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Approx budget</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="15000">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Requested on</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="15/12/2009">
                        </div>
                    </div>

                </form>
            </div>
            <div class="modal-footer">

                * Only for authorized Users of GPC
            </div>
        </div>
    </div>
</div>

