<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <p class="bg-success">Members of Meeting</p>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" role="form">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Meeting id</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" value="1">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Requested by</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="CS Department">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Member 1</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="name: Ali   designation: chair person GPC">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Member 2</label>
                        <div class="col-sm-10">
                            <input type="number" class="form-control" id="quant" placeholder="name: umer   designation: chair person CPU">
                        </div>
                    </div>

                    <div class="form-group">
